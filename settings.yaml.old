#geonex v.1.0
memory_allocation: 120000
max_performance: true
resize_to: 2048
enable_parallel_processing: true
batch_size: 1000
retry_on_failure: true
retry_count: 3
thread_count: 32
pc-quality: high
pc-las: True
pc-filter: True
pc-csv: True
orthopfoto-kmz: True
orthophoto-cutline: True
merge: 'all'
start_with: 'resize'
end_with: 'odm_postprocess'
zip_results: True
time: True
pc-quality: 'high'
sfm-algorithm: 'Incremental'
optimize-disk-space: True
pmvs_num_cores: 16 # by default this is set to $(nproc)
texturing_skip_hole_filling: False
dtm: True      # Use this tag to build a DTM (Digital Terrain Model
dsm: True      # Use this tag to build a DSM (Digital Surface Model
